    <panel>
      <title>GB Ingest Per Minute (Throughput is an estimate derived from "expected" ingest and search volumes, and should not be interpreted as actual)</title>
      <chart>
        <search>
          <query>index=_internal earliest=$global_range.earliest$ latest=$global_range.latest$
            host=idx*.nike.splunkcloud.com
            source=/opt/splunk/var/log/splunk/metrics.log
            sourcetype=splunkd
            TERM(group=tcpin_connections)
            component=Metrics 
            | eval b_span_units = kb*1024/1073741824
            | bucket _time span=1m
            | stats sum(b_span_units) as GB/m by _time 
            | timechart span=1m max(GB/m) | eval "Max Provisioned Throughput"=78, "Provisioned Throughput"=46</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
