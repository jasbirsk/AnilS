      <title>Queue Fill Perc90</title>
      <chart>
        <search>
          <query>index=_internal group=queue source=*metrics.log sourcetype=splunkd host=idx*.*splunk*.* OR host=si*.*splunk*.* name=aggqueue OR name=indexqueue OR name=parsingqueue OR name=splunktcpin OR name=typingqueue earliest=$global_range.earliest$ latest=$global_range.latest$
| eval max=if(isnotnull(max_size_kb),max_size_kb,max_size), curr=if(isnotnull(current_size_kb),current_size_kb,current_size), fill_perc=round(((curr / max) * 100),2) 
| timechart minspan=30s Perc90(fill_perc) AS "fill percentage" by name</query>
          <sampleRatio>1</sampleRatio>
        </search>
