    <panel>
      <title>Count of Skipped Searches Over Time</title>
      <chart>
        <search>
          <query>index=_internal host=sh*.*.splunkcloud.com sourcetype=scheduler (status="success" OR status="skipped" OR status="continued") search_type!="*_acceleration"   earliest=$global_range.earliest$ latest=$global_range.latest$
      | timechart count(eval(status=="success" OR status=="skipped")) AS total_exec, count(eval(status=="skipped")) AS skipped_exec
      | eval skip_ratio = round(skipped_exec / total_exec * 100, 2)</query>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
