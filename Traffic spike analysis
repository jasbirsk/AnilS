    <panel>
      <title>SVC Utilization</title>
      <chart>
        <title>Capacity Planning: SVC utilization is a trailing indicator based on search, ingest, and OS/app overhead.</title>
        <search>
          <query>index=_introspection component=Hostwide host="idx*" earliest=$global_range.earliest$ latest=$global_range.latest$
        |eval cpu_utilization=('data.cpu_user_pct' + 'data.cpu_system_pct')
        |bin _time span=1m
        |stats max(cpu_utilization) as cpu by host _time
        |rangemap field=cpu Low=0-65 Intermediate=65-75 High=75-100
        |rename range as load
        |timechart span=1m count(host) by load</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
